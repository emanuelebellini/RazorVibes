@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - Razor Vibes</title>
    <link rel="stylesheet" href="~/Content/Site.css" />
</head>
<body>
    <header class="site-header">
        <div class="container">
            <h1>Razor Vibes</h1>
            <nav>
                <a href="@Url.Action("Index", "Contacts")">Contacts</a>
                <a href="@Url.Action("Create", "Contacts")">New Contact</a>
            </nav>
        </div>
    </header>
    <main class="container">
        @RenderBody()
    </main>
    <footer class="site-footer">
        <div class="container">In-memory CRUD demo for ASP.NET MVC.</div>
    </footer>
</body>
</html>
